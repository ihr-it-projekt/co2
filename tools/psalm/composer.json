{
    "name": "tools/psalm-and-plugins",
    "description": "Psalm and phpunit plugin",
    "type": "metapackage",
    "require": {
        "php": "^8.1",
        "psalm/plugin-phpunit": "0.17.0",
        "vimeo/psalm": "4.23.0",
        "psalm/plugin-symfony": "^3.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "prefer-stable": true,
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        },
        "platform": {
            "php": "8.1.4"
        }
    }
}
